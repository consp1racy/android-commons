<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XpAppCompatCheckedTextView">
        <attr name="checkMark"/>
        <attr name="checkMarkTint"/>
        <attr name="checkMarkTintMode"/>
    </declare-styleable>

    <declare-styleable name="XpAppCompatCheckMarkHelper">
        <attr name="checkMark" format="reference"/>
        <attr name="checkMarkTint" format="color"/>
        <attr name="checkMarkTintMode" format="enum">
            <enum name="src_over" value="3"/>
            <enum name="src_in" value="5"/>
            <enum name="src_atop" value="9"/>
            <enum name="add" value="12"/>
            <enum name="multiply" value="14"/>
            <enum name="screen" value="15"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="XpAppCompatImageHelper">
        <attr name="android:enabled"/>
    </declare-styleable>

    <declare-styleable name="XpAppCompatImageView">
        <attr name="android:enabled"/>
    </declare-styleable>

    <declare-styleable name="XpAppCompatImageButton">
        <attr name="android:enabled"/>
    </declare-styleable>
</resources>
